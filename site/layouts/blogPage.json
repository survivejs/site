{
  "head": [
    {
      "element": "MetaFields"
    }
  ],
  "body": [
    {
      "element": "MainNavigation"
    },
    {
      "element": "header",
      "class": "w-full h-64 bg-cover bg-center",
      "children": [
        {
          "element": "h1",
          "class": "flex justify-center items-center text-white h-full text-5xl font-bold bg-gradient-to-b from-transparent to-gray-800",
          "__children": "match.data.title"
        }
      ],
      "attributes": {
        "==style": "'background-image:url(' + match.data.headerImage + ')'"
      }
    },
    {
      "element": "div",
      "class": "md:mx-auto my-8 px-4 md:px-0 w-full lg:max-w-3xl prose lg:prose-xl",
      "children": [
        {
          "element": "p",
          "inputProperty": "match.content",
          "transformWith": ["markdown"],
          "__children": "content"
        }
      ]
    },
    {
      "element": "MainFooter"
    },
    {
      "element": "Scripts",
      "__bind": "scripts"
    }
  ]
}
