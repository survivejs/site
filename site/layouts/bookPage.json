{
  "head": [
    {
      "element": "MetaFields"
    }
  ],
  "body": [
    {
      "element": "MainNavigation"
    },
    {
      "element": "aside",
      "class": "fixed top-16 pl-4 hidden lg:inline",
      "children": [
        {
          "element": "TableOfContents"
        }
      ]
    },
    {
      "element": "div",
      "class": "md:mx-auto my-8 px-4 md:px-0 w-full lg:max-w-3xl prose lg:prose-xl",
      "attributes": {
        "id": "main"
      },
      "children": [
        {
          "element": "h1",
          "__children": "match.data.title"
        },
        {
          "element": "p",
          "inputProperty": "match.content",
          "transformWith": ["markdown"],
          "__children": "content"
        }
      ]
    },
    {
      "element": "MainFooter"
    },
    {
      "element": "Scripts",
      "__bind": "scripts"
    }
  ]
}
