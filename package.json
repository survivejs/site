{
  "name": "survivejs-site",
  "private": true,
  "version": "2.0.0",
  "description": "SurviveJS site",
  "author": "Juho Vepsalainen <bebraw@gmail.com>",
  "scripts": {
    "start": "node ./antwar.bootstrap.js develop",
    "build": "rimraf build && npm run fetch:books && node ./antwar.bootstrap.js build && npm run sitemap",
    "deploy": "gh-pages -d build && npm run purge_cf",
    "fetch:books": "scripts/fetch-books.sh",
    "bootstrap": "scripts/bootstrap.sh",
    "sitemap": "cd build && sitemap-static --prefix=https://survivejs.com/ > sitemap.xml",
    "purge_cf": "node ./utils/purge_cf.js",
    "test": "npm run lint && npm run format",
    "check:links": "hyperlink build/index.html -r",
    "prepush": "npm test"
  },
  "dependencies": {
    "@survivejs/components": "^0.2.0",
    "emotion": "^8.0.12",
    "front-matter": "^2.3.0",
    "illuminate-js": "^1.0.0-alpha.1",
    "loader-utils": "^1.1.0",
    "lodash": "^4.17.19",
    "marked": "^0.3.9",
    "moment": "^2.20.1",
    "react": "^16.2.0",
    "react-emotion": "^8.0.12",
    "react-router-dom": "^4.2.2",
    "remove-markdown": "^0.2.2",
    "tamia-theme": "^0.1.0"
  },
  "devDependencies": {
    "antwar": "^0.21.1",
    "antwar-rss-plugin": "^0.21.0",
    "autoprefixer": "^7.2.3",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-emotion": "^8.0.12",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "cloudflare4": "^0.2.5",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.7",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "hyperlink": "^3.0.1",
    "json-loader": "^0.5.7",
    "node-sass": "^4.12.0",
    "postcss-loader": "^2.0.9",
    "raw-loader": "^0.5.1",
    "react-router": "^4.2.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^6.0.6",
    "serve-static": "^1.13.1",
    "sitemap-static": "^0.4.2",
    "style-loader": "^0.19.1",
    "title-case": "^2.1.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/survivejs/site.git"
  },
  "homepage": "https://www.survivejs.com",
  "bugs": {
    "url": "https://github.com/survivejs/site/issues"
  },
  "keywords": [
    "site",
    "antwar"
  ],
  "licenses": "MIT"
}
